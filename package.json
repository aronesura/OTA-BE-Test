{
  "name": "OTA-BE-Test",
  "version": "1.0.0",
  "description": "OTA Initial Coding Challenge",
  "main": "src/bin/index.ts",
  "license": "MIT",
  "author": {
    "name": "Aaron Esugerra",
    "email": "aronesguerra0508@gmail.com"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.20.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "start": "node build/bin/index.js",
    "clean": "rm -rf build",
    "build": "yarn clean && concurrently yarn:build:*",
    "build:compile": "npx babel src --extensions .ts --out-dir build --source-maps",
    "build:types": "tsc",
    "dev": "concurrently yarn:dev:* --kill-others \"nodemon --exec node build/bin/index.js\"",
    "dev:compile": "npx babel src --extensions .ts --out-dir build --source-maps --watch",
    "dev:types": "tsc --watch",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.23.9",
    "@babel/node": "7.23.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-typescript": "7.23.3",
    "@babel/runtime": "7.23.9",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.6",
    "@types/core-js": "2.5.8",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.11.19",
    "@types/regenerator-runtime": "0.13.5",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "babel-core": "6.26.3",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-source-map-support": "2.2.0",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "core-js": "3.36.0",
    "cors": "2.8.5",
    "dotenv": "16.4.4",
    "express": "4.18.2",
    "helmet": "7.1.0",
    "husky": "9.0.11",
    "nodemon": "3.0.3",
    "regenerator-runtime": "0.14.1"
  }
}
